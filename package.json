{
	"name": "dom-syntax-sugar",
	"description": "A pluggable babel plugin for adding syntax sugar to JSX.",
	"version": "1.0.10",
	"author": "Tito Bouzout",
	"main": "index.js",
	"type": "module",
	"types": "./index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/titoBouzout/dom-syntax-sugar.git"
	},
	"keywords": [
		"DOM",
		"Syntax Sugar",
		"JSX",
		"DOM Syntax Sugar",
		"JSX Syntax Sugar"
	],
	"bugs": {
		"url": "https://github.com/titoBouzout/dom-syntax-sugar/issues"
	},
	"homepage": "https://github.com/titoBouzout/dom-syntax-sugar#readme",
	"prettier": {
		"printWidth": 90,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"quoteProps": "as-needed",
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"arrowParens": "avoid",
		"proseWrap": "never",
		"endOfLine": "lf"
	},
	"dependencies": {
		"@babel/core": "^7.21.8",
		"solid-js": "^1.7.5"
	}
}
